{
  "name": "dams-server-side",
  "version": "1.0.0",
  "description": "Backend for disaster management system",
  "main": "index.js",
  "scripts": {
    "start": "node index",
    "server": "nodemon index",
    "test": "jest --forceExit  --runInBand --silent"
  },
  "jest": {
    "collectCoverage": true,
    "testRegex": "((\\.|/*.)(test))\\.js?$",
    "collectCoverageFrom": [
      "routes/*.{js,jsx}",
      "models/*.{js,jsx}",
      "middleware/*.{js,jsx}",
      "database/*.{js,jsx}"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/jspm_packages"
    ]
  },
  "author": "Asad Mahmood, Stefan Radovic and Dennis Kurtz",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^8.6.0",
    "express": "^4.17.1",
    "jest": "^26.6.3",
    "jsonwebtoken": "^9.0.2",
    "mysql": "^2.18.1",
    "mysql2": "^3.9.7",
    "nodemon": "^2.0.7",
    "sequelize": "^6.5.0",
    "sequelize-cli": "^6.2.0",
    "supertest": "^6.1.3"
  }
}
